#C:\Users\Yeeun\Desktop
import os

def FindDesktop():
    startPath = "c:\\Users\\"
    print(os.listdir(startPath))

    a = os.listdir(startPath)
    for i in a:
        if i == 'All Users':
            pass
        elif i == 'Default':
            pass
        elif i == 'Default User':
            pass
        elif i == 'desktop.ini':
            pass
        elif i == 'Public':
            pass
        else:
            startPath = startPath + i + '\\Desktop'
    if os.path.isdir(startPath):
        return startPath

print(FindDesktop())
